{
  "name": "escolalms/api",
  "description": "Headless LMS",
  "keywords": [
    "lms",
    "learning-management-system",
    "laravel",
    "reactjs"
  ],
  "homepage": "https://lms.escolasoft.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Wojczal",
      "email": "mateusz@wojczal.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "behat/mink-browserkit-driver": "^1.3",
    "bensampo/laravel-enum": "3.x",
    "darkaonline/l5-swagger": "^8.0.0",
    "dmore/behat-chrome-extension": "^1.3",
    "dmore/chrome-mink-driver": "^2.7",
    "doctrine/dbal": "^2",
    "escolalms/core": "*",
    "escolasoft/laravel-h5p": "dev-dev-qunabu-patch-3",
    "fideloper/proxy": "^4.0",
    "fruitcake/laravel-cors": "^2.0",
    "ignited/laravel-omnipay": "^3.0.2",
    "infyomlabs/adminlte-templates": "8.0.x-dev",
    "infyomlabs/laravel-generator": "8.0.x-dev",
    "intervention/image": "^2.5",
    "intervention/imagecache": "^2.5",
    "laravel/framework": "^8.0",
    "laravel/helpers": "^1.4",
    "laravel/legacy-factories": "^1.1",
    "laravel/passport": "^10.0",
    "laravel/socialite": "v5.1.3",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.0",
    "laravelcollective/html": "^6.2",
    "maatwebsite/excel": "^3.1",
    "mpdf/mpdf": "^8.0",
    "omnipay/paypal": "^3.0.2",
    "predis/predis": "^1.1",
    "sentry/sentry-laravel": "^2.3",
    "spatie/laravel-permission": "^3.18",
    "staudenmeir/eloquent-json-relations": "^1.5",
    "treestoneit/shopping-cart": "^1.4"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.9",
    "behat/behat": "^3.8",
    "behat/mink": "^1.8",
    "beyondcode/laravel-dump-server": "^1.0",
    "brainmaestro/composer-git-hooks": "^2.8",
    "facade/ignition": "^2.0",
    "filp/whoops": "^2.0",
    "friends-of-behat/mink-extension": "^2.4",
    "friendsofphp/php-cs-fixer": "^2.16",
    "fzaninotto/faker": "^1.4",
    "infyomlabs/swagger-generator": "dev-master",
    "laracasts/behat-laravel-extension": "dev-master as 1.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "php-mock/php-mock-phpunit": "^2.6",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "hooks": {
      "pre-commit": [
        "echo committing as $(git config user.name)",
        "php-cs-fixer fix ."
      ]
    }
  },
  "autoload": {
    "files": [
      "app/Http/SiteHelpers.php",
      "app/Http/Controllers/Overwrite/EmbedController.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories",
      "Database\\Seeders\\": "database/seeds"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Features\\": "features/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "make link"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "siffer": [
      "./vendor/bin/php-cs-fixer fix ."
    ],
    "behat": [
      "./vendor/bin/behat"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/MWL91/Behat-Laravel-Extension"
    },
    {
      "type": "vcs",
      "url": "https://github.com/EscolaSoft/h5p-php-library"
    },
    {
      "type": "vcs",
      "url": "https://github.com/EscolaLMS/Core"
    }
  ]
}
